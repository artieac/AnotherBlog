<div id="blogListSection" ng-controller="ListController" >
	<div class="navigationSection" ng-init="getAllBlogs();">
        <div class="navigationSectionTitle">
            <label>{{ allBlogs.Title }}</label>
        </div> 
        <div class="navigationSectionBody" >
            <ul class="navigationSectionList">
                <li class="navigationSectionItem" ng-repeat="blogItem in allBlogs.ListItems">
                    <a href='/Blog/{{blogItem.RelatedLink}}/BlogPosts'>{{blogItem.Name}}</a>
                </li>
            </ul>
        </div>
    </div>
</div>